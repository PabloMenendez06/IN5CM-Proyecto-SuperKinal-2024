<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="750.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.pablomenendez.controller.FormLoginController">
   <children>
      <ImageView fitHeight="750.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/FondoLogin.png" />
         </image>
      </ImageView>
      <TextField fx:id="tfUser" layoutX="14.0" layoutY="155.0" prefHeight="37.0" prefWidth="334.0" />
      <PasswordField fx:id="tfPassword" layoutX="20.0" layoutY="336.0" prefHeight="38.0" prefWidth="334.0" />
      <Button fx:id="btnIniciar" layoutX="173.0" layoutY="516.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="56.0" prefWidth="150.0" text="Iniciar Sesion">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="btnRegistrar" disable="true" layoutX="173.0" layoutY="599.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="56.0" prefWidth="150.0" scaleShape="false" text="Registrar usuario">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
